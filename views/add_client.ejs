<% include ./partials/header.ejs%>

<div class="page-header"><%=header%></div>

<div class="container">
    <h1 class="text-center">Welcome to Migrant med</h1>

    <form action="/clients/add" method="POST">

        <div class="form-group">
            <label for="">Name:</label>
            <input type="text" placeholder="Full Name" class="form-control" name="name">
        </div>

        <div class="form-group">
            <label for="">Phone:</label>
            <input type="text" placeholder="Phone" class="form-control" name="phone">
        </div>

        <div class="form-group">
            <label for="">Address:</label>
            <input type="text" placeholder="Full Name" class="form-control" name="address">
        </div>

        <div class="form-group">
            <label for="">Address 2:</label>
            <input type="text" placeholder="Address 2" class="form-control" name="address2">
        </div>

        <div class="form-group">
            <label for="">State:</label>
            <input type="text" placeholder="State" class="form-control" name="state">
        </div>

        <div class="form-group">
            <label for="">City:</label>
            <input type="text" placeholder="City" class="form-control" name="city">
        </div>

        <div class="form-group">
            <label for="">Zip code:</label>
            <input type="text" placeholder="Zip Code" class="form-control" name="zip">
        </div>

        <div class="form-group">
            <label for="">Years of experience:</label>
            <input type="text" placeholder="Zip Code" class="form-control" name="yoe">
        </div>

        <div class="form-group">
            <label for="">Specialty:</label>
            <input type="text" placeholder="Zip Code" class="form-control" name="specialty">
        </div>

        <input type="submit" value="Submit">

    </form>


</div>

<% include ./partials/footer %>